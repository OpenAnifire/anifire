<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
	xmlns:s="library://ns.adobe.com/flex/spark" 
	xmlns:fb="http://ns.adobe.com/flashbuilder/2009">

	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata>

    <s:states>
        <s:State name="up"/>
        <s:State name="over"/>
        <s:State name="down"/>
        <s:State name="disabled"/>
    </s:states>

    <s:Group>
		<s:BitmapImage source="@Embed('../images/manType_normal.png')" smooth="true" verticalCenter="0" horizontalCenter="0"/>
		<s:BitmapImage source="@Embed('../images/manType_hover.png')" visible="false" visible.over="true" smooth="true" verticalCenter="0" horizontalCenter="0"/>
	</s:Group>
    
</s:Skin>
