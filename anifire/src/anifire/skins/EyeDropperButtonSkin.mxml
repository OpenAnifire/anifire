<?xml version="1.0" encoding="utf-8"?>
<s:SparkButtonSkin
	xmlns:fx="http://ns.adobe.com/mxml/2009" 
	xmlns:s="library://ns.adobe.com/flex/spark" 
	xmlns:mx="library://ns.adobe.com/flex/mx">

	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>

	<s:BitmapImage
		verticalCenter="0"
		horizontalCenter="0"
		source="@Embed('../images/colorPicker/eyedropper.png')"
		source.over="@Embed('../images/colorPicker/eyedropper_active.png')"
		alpha.disabled="0.5"/>

</s:SparkButtonSkin>
