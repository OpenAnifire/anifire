<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin
	xmlns:fx="http://ns.adobe.com/mxml/2009" 
	xmlns:s="library://ns.adobe.com/flex/spark" 
	xmlns:mx="library://ns.adobe.com/flex/mx"
	minWidth="20"
	minHeight="20"
	alpha.disabled="0.5">

	<s:states>
		<s:State name="up"/>
		<s:State name="over" stateGroups="overAndDownStates"/>
		<s:State name="down" stateGroups="overAndDownStates"/>
		<s:State name="disabled"/>
	</s:states>

	<s:Ellipse height="25" width="25">
		<s:stroke>
			<s:SolidColorStroke color="0xBBBBBB"/>
		</s:stroke>
		<s:fill>
			<s:SolidColor color="0xBBBBBB"/>
		</s:fill>
	</s:Ellipse>
	<s:Rect height="25" width="22" x="13" radiusX="4" radiusY="4">
		<s:fill>
			<s:SolidColor color="0xBBBBBB"/>
		</s:fill>
	</s:Rect>
	<s:Rect id="frameDisplay" left="0" right="0" top="0" bottom="0" radiusX="4" radiusY="4"/>
	<s:Rect id="shadow" left="1" top="1" right="1" bottom="1" radiusX="3"/>

</s:SparkSkin>
