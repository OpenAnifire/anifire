<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin
	xmlns:fx="http://ns.adobe.com/mxml/2009" 
	xmlns:s="library://ns.adobe.com/flex/spark" 
	xmlns:mx="library://ns.adobe.com/flex/mx">

	<fx:Metadata>
		[HostComponent("spark.components.VSlider")]
	</fx:Metadata>

	<s:states>
		<s:State name="disabled"/>
		<s:State name="normal"/>
	</s:states>
	
	<s:Button
		id="track"
		left="0"
		right="0"
		top="0"
		bottom="0"
		minHeight="33"
		height="100"
		tabEnabled="false"
		skinClass="anifire.skins.ColorInputPanelHueSliderTrackSkin"/>
	<s:Button
		id="thumb"
		width="25"
		height="25"
		tabEnabled="false"
		skinClass="anifire.skins.ColorInputPanelHueSliderThumbSkin"/>

</s:SparkSkin>
